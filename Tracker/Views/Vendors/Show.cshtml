@using Tracker.Models;

<h2>Orders under @Model["vendor"].Name:</h2>

<ol>
@foreach (Order orders in @Model["orders"])
{
    <li><a href="/vendors/@Model["vendor"].Id/orders/@orders.Id">@orders.Title</a></li>
}
</ol>

<p><a href="/vendors/@Model["vendor"].Id/orders/new">Add a new order under this vendor</a></p>
<p><a href="/vendors">Return to vendor list</a></p>
<p><a href="/">Return to home page</a></p>